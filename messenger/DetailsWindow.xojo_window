#tag WindowCode
	#tag Event
		Sub Opening()
		  #If TargetMacOS
		    Const NSTitledWindowMask = 1
		    Const NSClosableWindowMask = 2
		    Const NSResizableWindowMask = 8
		    
		    Declare Sub setStyleMask Lib "Cocoa.framework" selector "setStyleMask:" (obj_id As Integer, mask As Integer)
		    
		    Var WinHandle as OSHandle
		    WinHandle = Self.Handle
		    
		    setStyleMask(WinHandle, NSTitledWindowMask Or NSClosableWindowMask Or NSResizableWindowMask)
		  #EndIf
		  
		  DetailTabPanel.RemovePanelAt(3)
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Sub ReadResultDetails(id As Integer)
		  Try
		    
		    Var sql As String = "SELECT * FROM result WHERE id = ?"
		    Var rs As RowSet = DB.SelectSQL(sql, id)
		    
		    ' Search item result details
		    
		    NameField.Text = rs.Column("name").StringValue
		    DescriptionField.Text = rs.Column("description").StringValue
		    UrlField.Text = rs.Column("url").StringValue
		    ProductIdField.Text = rs.Column("product_id").StringValue
		    ImageField.Text = rs.Column("image").StringValue
		    BrandField.Text = rs.Column("brand").StringValue
		    TimestampField.Text = rs.Column("timestamp").StringValue
		    
		    ' Offer details
		    PriceField.Text = rs.Column("offer_price").StringValue
		    CurrencyField.Text = rs.Column("offer_currency").StringValue
		    ConditionField.Text = rs.Column("offer_condition").StringValue
		    AvailabilityField.Text = rs.Column("offer_availability").StringValue
		    
		    ' Seller details
		    SellerField.Text = rs.Column("seller_name").StringValue
		    SellerUrlField.Text = rs.Column("seller_url").StringValue
		    SellerImageField.Text = rs.Column("seller_image").StringValue
		    
		    ' Item rating details
		    BestRatingField.Text = rs.Column("aggregate_best_rating").StringValue
		    WorstRatingField.Text = rs.Column("aggregate_worst_rating").StringValue
		    RatingCountField.Text = rs.Column("aggregate_rating_count").StringValue
		    RatingValueField.Text = rs.Column("aggregate_rating_value").StringValue
		    
		    rs.Close
		    
		  Catch err As DatabaseException
		    
		    MessageDialog.Show("DB Error: " + err.Message)
		    Return
		    
		  End Try
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub SetDetails(id As Integer)
		  Self.resultId = id
		  ReadResultDetails(id)
		  
		  Var itemImage As SearchDatabase.ItemImage = ReadItemImageByResult(id)
		  
		  If itemImage <> Nil And Not itemImage.IsEmpty Then
		    ItemImageViewer.Image = itemImage.ToPicture
		  Else
		    ImageDownloadButton.Visible = True
		  End If
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h21
		Private resultId As Integer
	#tag EndProperty


