#tag Events ImageDownloadButton
	#tag Event
		Sub Pressed()
		  Var imageURL As SearchServiceURL = New SearchServiceURL(App.ServiceURL, App.apiToken)
		  
		  imageURL.SiteURL = ImageField.Text
		  imageURL.RenderJS = False
		  imageURL.RichRespone = True
		  
		  WebConnection.Send("GET", imageURL.ToString)
		  
		  DownloadProgressWheel.Visible = True
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events WebConnection
	#tag Event
		Sub ContentReceived(URL As String, HTTPStatus As Integer, content As String)
		  #PRAGMA unused URL
		  
		  If HTTPStatus <> 200 Then
		    MessageBox(HTTPStatus.ToString + content)
		    Return
		  End If
		  
		  Var itemImage As SearchDatabase.ItemImage = New SearchDatabase.ItemImage(content)
		  SearchDatabase.CreateItemImage(itemImage, resultId)
		  
		  Var picture As Picture = itemImage.ToPicture
		  
		  If picture <> Nil Then
		    ItemImageViewer.Image = itemImage.ToPicture
		    ImageDownloadButton.Visible = False
		  End If
		  
		  DownloadProgressWheel.Visible = False
		End Sub
	#tag EndEvent
	#tag Event
		Sub Error(e As RuntimeException)
		  MessageBox(e.Message)
		  DownloadProgressWheel.Visible = False
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ItemDownloadButton
	#tag Event
		Sub Pressed()
		  Var siteURL As String = ImageField.Text
		  siteURL = EncodeURLComponent(siteURL)
		  
		  Var url As String = "https://app.scrapingbee.com/api/v1/?api_key=" + App.apiToken + "&url="+ siteURL + "&render_js=false&json_response=true"
		  
		  WebConnection.Send("GET", url)
		  
		  DownloadProgressWheel.Visible = True
		End Sub
	#tag EndEvent
#tag EndEvents
