#tag WindowCode
	#tag Event
		Sub Opening()
		  sessionID = Session.Identifier
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub EnableControls()
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub EnableSaveButton()
		  If PasswordTextField.Text = ConfirmationTextField.Text Then
		    SaveButton.Enabled = True
		    PasswordWarningLabel.Visible = False
		  Else
		    SaveButton.Enabled = False
		    PasswordWarningLabel.Visible = True
		  End If
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub SetParent(parent As SettingsContainer)
		  settingsContainer = parent
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub SetUserDetails(index As Integer, userNo As Integer, username As String, email As String, name As String, role As String)
		  UserNameTextField.ReadOnly = True
		  
		  id = userNo
		  row = index
		  
		  UserNameTextField.Text = username
		  EmailTextField.Text = email
		  NameTextField.Text = name
		  RoleCombobox.Text = role
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h21
		Private id As Integer = 0
	#tag EndProperty

