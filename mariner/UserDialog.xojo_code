#tag Events SaveButton
	#tag Event
		Sub Pressed()
		  Var user As New JSONItem
		  
		  user.Value("username") = UserNameTextField.Text
		  user.Value("name") = NameTextField.Text
		  user.Value("email") = EmailTextField.Text
		  user.Value("role") = RoleCombobox.SelectedRowText
		  
		  user.Value("password") = PasswordTextField.Text
		  user.Value("confirmation") = ConfirmationTextField.Text
		  
		  Var serviceURL As ServiceURL = New ServiceURL(App.ServiceBaseURL)
		  serviceURL.Path = "/user"
		  
		  UserConnection.SetRequestContent(user.ToString, "application/json")
		  
		  Var method As String
		  
		  If id = 0 Then
		    method = "POST"
		    serviceURL.Path = "/user"
		  Else
		    method = "PUT"
		    serviceURL.Path = "/user/" + id.ToString
		  End If
		  
		  'DisableControls
		  'SearchProgressWheel.Visible = True
		  
		  UserConnection.Send(method, serviceURL.ToString, 240)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CancelButton
	#tag Event
		Sub Pressed()
		  Self.Close
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events UserConnection
	#tag Event
		Sub ContentReceived(URL As String, HTTPStatus As Integer, content As String)
		  If id = 0 Then
		    settingsContainer.RefreshUserList
		  Else
		    Var user As New JSONItem
		    
		    user.Value("username") = UserNameTextField.Text
		    user.Value("name") = NameTextField.Text
		    user.Value("email") = EmailTextField.Text
		    user.Value("role") = RoleCombobox.SelectedRowText
		    
		    settingsContainer.UpdateUserDetails(row, user)
		  End If
		  
		  Self.Close
		End Sub
	#tag EndEvent
	#tag Event
		Sub Error(e As RuntimeException)
		  EnableControls
		  'SearchProgressWheel.Visible = False
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function AuthenticationRequested(realm As String, ByRef name As String, ByRef password As String) As Boolean
		  Var context As New WebSessionContext(sessionID)
		  name = Session.Username
		  password = Session.Password
		  Return True
		End Function
	#tag EndEvent
#tag EndEvents
