#tag WindowCode
	#tag Event
		Sub Opening()
		  #If TargetMacOS
		    Const NSTitledWindowMask = 1
		    Const NSClosableWindowMask = 2
		    Const NSResizableWindowMask = 8
		    
		    Declare Sub setStyleMask Lib "Cocoa.framework" selector "setStyleMask:" (obj_id As Integer, mask As Integer)
		    
		    Var WinHandle as OSHandle
		    WinHandle = Self.Handle
		    
		    setStyleMask(WinHandle, NSTitledWindowMask Or NSClosableWindowMask)
		  #EndIf
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub Constructor(title As String, description As String, link As String)
		  // Calling the overridden superclass constructor.
		  Super.Constructor
		  
		  TitleTextField.Text = title
		  DescriptionTextArea.Text = description
		  UrlTextField.Text = link
		End Sub
	#tag EndMethod


#tag EndWindowCode

#tag Events CloseButton
	#tag Event
		Sub Pressed()
		  Self.Close
		End Sub
	#tag EndEvent
#tag EndEvents
